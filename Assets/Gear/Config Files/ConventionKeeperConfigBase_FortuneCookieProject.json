{
  "active":true,
  "folderStructure": {
    "check":{
      "folders": [
		{
		  "path": "Assets",
          "fileTypesAllowed": ["folder"]
		},
		{
		  "path": "Assets/Art",
          "fileTypesAllowed": ["folder"]
		},
		{
		  "path": "Assets/Art/Models",
          "fileTypesAllowed": ["folder"]
		},
		{
		  "path": "Assets/Art/Models/Others",
          "fileTypesAllowed": ["folder", "fbx", "obj"]
		},
		{
		  "path": "Assets/Art/ParticleSystems",
          "fileTypesAllowed": ["folder"]
		},
		{
		  "path": "Assets/Art/Animations",
          "fileTypesAllowed": ["folder"]
		},
		{
		  "path": "Assets/Art/Animations/Others",
          "fileTypesAllowed": ["folder", "anim"]
		},
		{
		  "path": "Assets/Art/Materials",
          "fileTypesAllowed": ["folder"]
		},
		{
		  "path": "Assets/Art/Materials/Models",
          "fileTypesAllowed": ["folder", "mat"]
		},
		{
		  "path": "Assets/Art/Materials/VFX",
          "fileTypesAllowed": ["folder", "mat"]
		},
		{
		  "path": "Assets/Art/Materials/Fonts",
          "fileTypesAllowed": ["folder", "mat"]
		},
		{
		  "path": "Assets/Art/Materials/Others",
          "fileTypesAllowed": ["folder", "mat"]
		},
		{
		  "path": "Assets/Art/Shaders",
          "fileTypesAllowed": ["folder"]
		},
		{
		  "path": "Assets/Art/Shaders/Others",
          "fileTypesAllowed": ["folder","shader"]
		},
		{
		  "path": "Assets/Art/Textures",
          "fileTypesAllowed": ["folder"]
		},
		{
		  "path": "Assets/Art/Textures/UI",
          "fileTypesAllowed": ["folder","png","jpg"]
		},
		{
		  "path": "Assets/Art/Textures/VFX",
          "fileTypesAllowed": ["folder","png","jpg"]
		},
		{
		  "path": "Assets/Art/Textures/Others",
          "fileTypesAllowed": ["folder","png","jpg"]
		},
		{
		  "path": "Assets/Art/FontsRaw",
          "fileTypesAllowed": ["otf","ttf"]
		},
		{
		  "path": "Assets/Prefabs",
          "fileTypesAllowed": ["folder"]
		},
		{
		  "path": "Assets/Prefabs/FortuneCookie",
          "fileTypesAllowed": ["folder"]
		},
		{
		  "path": "Assets/Scenes",
          "fileTypesAllowed": ["unity"]
		}
      ]
    },
    "ignore":{
      "folders": [
		"Assets/AssetBundles",
		"Assets/AutomationTests",
		"Assets/Benchmarker",
		"Assets/DLC",
		"Assets/Editor",
		"Assets/Editor Default Resources",
		"Assets/GooglePlayGames",
		"Assets/MojoGUI",
		"Assets/NativeiOSSupport",
		"Assets/PlayServicesResolver",
		"Assets/Plugins",
		"Assets/Resources",
		"Assets/ScopelyPlatform",
		"Assets/ScopelySDKUpdater",
		"Assets/Scripts",
		"Assets/Spine",
		"Assets/StoreArt",
		"Assets/StreamingAssets",
		"Assets/TextMesh Pro",
		"Assets/Tests",
		"Assets/Voltron",
		"Assets/WithBuddies",
		"Assets/Gear"
      ],
      "fileTypes": [
		"meta",
		"xml",
		"rsp",
		"DS_Store"
	  ]
    }
  },
  "namingConvention": {
    "fileTypes":[
      {
        "types": ["fbx","obj"],
        "conventions": [
          "{modelName}.{extension}"
        ]
      },
      {
        "types": ["anim"],
        "conventions": [
          "{usage}_{animationName}_Anim.{extension}",
          "{usage}_Anim_{animationName}.{extension}"
        ]
      },
	  {
        "types": ["controller"],
        "conventions": [
          "{usage}_{controllerName}.{extension}"
        ]
      },
      {
        "types": ["jpg","png"],
        "conventions": [
          "{usage}_{textureName}.{extension}"
        ]
      },
      {
        "types": ["mat"],
        "conventions": [
          "{usage}_{materialName}.{extension}"
        ]
      },,
	  {
		"types": ["cs"],
        "conventions": [
          "{scriptName}.{extension}"
        ]
	  },
	  {
		"types": ["unity"],
        "conventions": [
          "{sceneName}Scene.{extension}"
        ]
	  },
	  {
		"types": ["shader"],
        "conventions": [
          "{shaderName}.{extension}"
        ]
	  },
	  {
		"types": ["prefab"],
        "conventions": [
          "{prefabName}_Prefab.{extension}"
        ]
	  },
	  {
		"types": ["otf","ttf"],
        "conventions": [
          "{fontName}.{extension}"
        ]
	  }
    ],
    "conventionKeyRules":[
      {
        "key":"{animationName}",
        "rules": [
		  "sufix(_Anim",
		  "prefix(Anim_"
        ]
      },
	  {
        "key":"{extension}",
        "rules": [
          "exact(.",
		  "is(jpg",
		  "is(png",
		  "is(shader",
		  "is(prefab",
		  "is(anim",
		  "is(mat",
		  "is(controller",
		  "is(cs",
		  "is(unity",
		  "is(fbx",
		  "is(obj",
		  "is(otf",
		  "is(ttf"
        ]
      },
	  {
	    "key":"{fontName}",
		"rules" : [
			"text"
		]
	  },
	  {
        "key":"{prefabName}",
        "rules": [
          "sufix(_Prefab"
        ]
      },
	  {
        "key":"{usage}",
        "rules": [
          "sufix(_"
        ]
      },
	  {
	    "key":"{modelName}",
		"rules" : [
			"text"
		]
	  },
	  {
	    "key":"{textureName}",
		"rules" : [
			"text"
		]
	  },
	  {
	    "key":"{controllerName}",
		"rules" : [
			"text"
		]
	  },
	  {
	    "key":"{scriptName}",
		"rules" : [
			"text"
		]
	  },
	  {
	    "key":"{sceneName}",
		"rules" : [
			"sufix(Scene"
		]
	  },
	  {
	    "key":"{shaderName}",
		"rules" : [
			"text"
		]
	  },
	  {
	    "key":"{materialName}",
		"rules" : [
			"text"
		]
	  }
    ],
	"regexDictionary":[
	  {
	    "function":"prefix",
		"regex":"KEY\w+"
	  },
	  {
	    "function":"sufix",
		"regex":"\w+KEY"
	  },
	  {
	    "function":"is",
		"regex":"KEY"
	  },
	  {
	    "function":"exact",
		"regex":"KEY"
	  },
	  {
	    "function":"text",
		"regex":"\w+"
	  }
	]
  }
}